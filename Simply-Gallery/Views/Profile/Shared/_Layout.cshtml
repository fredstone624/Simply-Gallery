@{
    ViewBag.MainTitle = "Профиль";
}

<!DOCTYPE HTML>
<html>
<head>
    <title>@ViewBag.MainTitle - @ViewBag.Title</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    @RenderSection("Style", required: false)

    <link href="@Url.Content("~/Content/home/vendor/bootstrap/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/profile/css/all.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/profile/css/main.css")" rel="stylesheet" type="text/css" />

</head>
<body>

    @{
        Html.RenderAction("Header", "Profile");
    }

    <!-- Main -->
    <div id="main">

        <div id="property">
            @RenderBody()
        </div>

        <!-- Footer -->
        <div id="footer">
            <ul class="copyright">
                <li>&copy; Simply Gallery</li>
                <li>Developer: Roman Osipyk</li>
            </ul>
        </div>

    </div>

    <!-- Scripts -->
    <script src="@Url.Content("~/Content/home/vendor/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/home/vendor/bootstrap/js/bootstrap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/home/vendor/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/profile/js/jquery.scrolly.min.js")"></script>
    <script src="@Url.Content("~/Content/profile/js/breakpoints.min.js")"></script>
    <script src="@Url.Content("~/Content/home/vendor/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/profile/js/util.min.js")"></script>
    <script src="@Url.Content("~/Content/profile/js/jquery.lazyload.min.js")"></script>

    @RenderSection("Script", required: false)

    <script src="@Url.Content("~/Content/profile/js/main.js")"></script>

</body>
</html>
@{ 
    ViewBag.MainTitle = "Simply Gallery";
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.MainTitle - @ViewBag.Title</title>
    <meta content="width=device-width, initial-scale=1" name="viewport" />

    <!-- GLOBAL MANDATORY STYLES -->
    <link href="~/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" type="text/css" /> <!-- bootstrap -->

    <!-- PAGE LEVEL PLUGIN STYLES -->
    <link href="~/Content/css/animate.css" rel="stylesheet"> <!-- cross-browser animations -->
    <link href="~/Content/vendor/swiper/css/swiper.min.css" rel="stylesheet" type="text/css" /> <!-- mobile touch slider -->
    <link href="~/Content/vendor/magnific-popup/magnific-popup.css" rel="stylesheet" type="text/css" /> <!-- for image gallery -->
    
    <!-- THEME STYLES -->
    <link href="~/Content/css/layout-main.min.css" rel="stylesheet" type="text/css"/> <!-- layout style -->
</head>
<body id="home" data-spy="scroll" data-target=".header">

    <!-- Render body -->
    @RenderBody()

    <!-- Back To Top -->
    <a href="javascript:void(0);" class="js-back-to-top back-to-top">Вверх</a>

    <!-- CORE PLUGINS -->
    <script src="~/Content/vendor/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/vendor/jquery-migrate.min.js" type="text/javascript"></script>
    <script src="~/Content/vendor/bootstrap/js/bootstrap.js" type="text/javascript"></script>

    <!-- PAGE LEVEL PLUGINS -->
    <script src="~/Content/vendor/jquery.easing.js" type="text/javascript"></script>
    <script src="~/Content/vendor/jquery.back-to-top.js" type="text/javascript"></script>
    <script src="~/Content/vendor/jquery.smooth-scroll.js" type="text/javascript"></script>
    <script src="~/Content/vendor/jquery.wow.min.js" type="text/javascript"></script>
    <script src="~/Content/vendor/swiper/js/swiper.jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/vendor/magnific-popup/jquery.magnific-popup.min.js" type="text/javascript"></script>
    <script src="~/Content/vendor/masonry/jquery.masonry.pkgd.min.js" type="text/javascript"></script>
    <script src="~/Content/vendor/masonry/imagesloaded.pkgd.min.js" type="text/javascript"></script>
    <script src="~/Content/vendor/jquery.unobtrusive-ajax.min.js" type="text/javascript"></script>
    <script src="~/Content/vendor/jquery.easing.js" type="text/javascript"></script>
    <script src="~/Content/vendor/jquery.validate.min.js" type="text/javascript"></script>

    <!-- PAGE LEVEL SCRIPTS -->
    <script src="~/Content/js/layout.min.js" type="text/javascript"></script>
    <script src="~/Content/js/components/wow.min.js" type="text/javascript"></script>
    <script src="~/Content/js/components/swiper.min.js" type="text/javascript"></script>
    <script src="~/Content/js/components/maginific-popup.min.js" type="text/javascript"></script>
    <script src="~/Content/js/components/masonry.min.js" type="text/javascript"></script>

</body>
</html>